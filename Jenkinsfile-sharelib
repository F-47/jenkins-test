@Library('jenkins-shared-lib@main') _

pipeline {
    agent any
    environment {
        IMAGE_NAME = "executioner47/python-iti"
    }
    stages {
        stage('Build') {
            steps {
                sh "mvn clean package"
            }
        }
        stage('Docker') {
            steps {
                dockerPush("${IMAGE_NAME}")
            }
        }
    }
}
